          <packing>
            <property name="x">270</property>
            <property name="y">425</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lbl_Jogo">
            <property name="width_request">50</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Jogos</property>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">90</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBox" id="cb_Jogos">
            <property name="width_request">200</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="model">list_Jogos</property>
            <property name="active">0</property>
            <child>
              <object class="GtkCellRendererText" id="cellrenderertext_Jogos"/>
              <attributes>
                <attribute name="text">0</attribute>
              </attributes>
            </child>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">110</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lbl_Jogador">
            <property name="width_request">50</property>
            <property name="height_request">22</property>
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="yalign">0</property>
            <property name="label" translatable="yes">Jogador</property>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">150</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBox" id="cb_Jogador">
            <property name="width_request">200</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="model">list_Jogadores</property>
            <child>
              <object class="GtkCellRendererText" id="cellrenderertext_Jogadores"/>
              <attributes>
                <attribute name="text">0</attribute>
              </attributes>
            </child>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">170</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btn_Cadastrar">
            <property name="label" translatable="yes">Cadastrar novo Jogador</property>
            <property name="width_request">150</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="events">GDK_BUTTON_PRESS_MASK | GDK_STRUCTURE_MASK</property>
            <property name="related_action">Cadastrar</property>
          </object>
          <packing>
            <property name="x">150</property>
            <property name="y">215</property>
          </packing>
        </child>
        <child>
