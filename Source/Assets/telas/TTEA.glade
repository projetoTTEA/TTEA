      <row>
        <col id="0" translatable="yes">KarTEA</col>
      </row>
      <row>
        <col id="0" translatable="yes">RepeTEA</col>
      </row>
    </data>
  </object>
  <object class="GtkAction" id="Cadastrar"/>
