<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="wd_Menu">
    <property name="resizable">False</property>
    <property name="window_position">center-on-parent</property>
    <child>
      <object class="GtkFixed" id="container">
        <property name="width_request">450</property>
        <property name="height_request">550</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="lblTTEA">
            <property name="width_request">100</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">Torre TEA</property>
          </object>
          <packing>
            <property name="x">175</property>
            <property name="y">30</property>
          </packing>
        </child>
        <child>
          <object class="GtkImage" id="img_udesc">
            <property name="width_request">150</property>
            <property name="height_request">50</property>
            <property name="visible">True</property>
            <property name="pixbuf">udesc.png</property>
          </object>
          <packing>
            <property name="x">30</property>
            <property name="y">450</property>
          </packing>
        </child>
        <child>
          <object class="GtkImage" id="img_larva">
            <property name="width_request">150</property>
            <property name="height_request">100</property>
