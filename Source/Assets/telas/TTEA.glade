            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="yalign">0</property>
            <property name="label" translatable="yes">Fase</property>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">305</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBox" id="cb_Fase">
            <property name="width_request">75</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="model">list_Niveis</property>
            <child>
              <object class="GtkCellRendererText" id="cellrenderertext_Fases"/>
              <attributes>
                <attribute name="text">0</attribute>
              </attributes>
            </child>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">325</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lbl_Nivel">
            <property name="width_request">40</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="xalign">0</property>
            <property name="yalign">0</property>
            <property name="label" translatable="yes">N&#xED;vel</property>
          </object>
          <packing>
            <property name="x">250</property>
            <property name="y">305</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBox" id="cb_Nivel">
            <property name="width_request">75</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="sensitive">False</property>
